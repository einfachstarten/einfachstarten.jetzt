<script>
    document.addEventListener('DOMContentLoaded', function() {
        const navToggle = document.querySelector('.nav-toggle');
        const navLinks = document.querySelector('.nav-links');
        const navLinkItems = document.querySelectorAll('.nav-links a');
        navToggle.addEventListener('click', function() {
            navLinks.classList.toggle('active');
        });
        navLinkItems.forEach(function(link) {
            link.addEventListener('click', function() {
                navLinks.classList.remove('active');
            });
        });
        let lastScrollTop = 0;
        const navbar = document.querySelector('nav');
        window.addEventListener('scroll', function() {
            const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
            if (scrollTop > lastScrollTop && scrollTop > 100) {
                navbar.classList.add('hide');
            } else {
                navbar.classList.remove('hide');
                if (scrollTop > 50) {
                    navbar.classList.add('shrink');
                } else {
                    navbar.classList.remove('shrink');
                }
            }
            lastScrollTop = scrollTop;
        });
        function duplicateContent(wrapper) {
            while (wrapper.scrollWidth < window.innerWidth * 2) {
                wrapper.innerHTML += wrapper.innerHTML;
            }
        }
        const skillsWrapper = document.getElementById('skillsWrapper');
        const techWrapper = document.getElementById('techWrapper');
        let positionSkills = 0;
        let speedSkills = 0.5;
        let positionTech = 0;
        let speedTech = 0.6;
        duplicateContent(skillsWrapper);
        duplicateContent(techWrapper);
        function animateSkills() {
            positionSkills -= speedSkills;
            if (positionSkills <= -skillsWrapper.scrollWidth / 2) {
                positionSkills = 0;
            }
            skillsWrapper.style.transform = `translateX(${positionSkills}px)`;
            requestAnimationFrame(animateSkills);
        }
        document.getElementById('scrollContainerSkills').addEventListener('mouseenter', () => {
            speedSkills = 0.2;
        });
        document.getElementById('scrollContainerSkills').addEventListener('mouseleave', () => {
            speedSkills = 0.5;
        });
        function animateTech() {
            positionTech -= speedTech;
            if (positionTech <= -techWrapper.scrollWidth / 2) {
                positionTech = 0;
            }
            techWrapper.style.transform = `translateX(${positionTech}px)`;
            requestAnimationFrame(animateTech);
        }
        document.getElementById('scrollContainerTech').addEventListener('mouseenter', () => {
            speedTech = 0.3;
        });
        document.getElementById('scrollContainerTech').addEventListener('mouseleave', () => {
            speedTech = 0.6;
        });
        animateSkills();
        animateTech();
    });


document.addEventListener('DOMContentLoaded', function() {
    const darkModeToggle = document.getElementById('darkModeToggle');
    const darkModeOverlay = document.getElementById('darkModeOverlay');

    // Toggle dark mode overlay
    darkModeToggle.addEventListener('click', function() {
        darkModeOverlay.classList.toggle('active');
    });

    // Update flashlight position on mouse move
    document.addEventListener('mousemove', function(event) {
        if (darkModeOverlay.classList.contains('active')) {
            const x = event.clientX + 'px';
            const y = event.clientY + 'px';
            darkModeOverlay.style.setProperty('--mouse-x', x);
            darkModeOverlay.style.setProperty('--mouse-y', y);
        }
    });
});
</script>

<div id="darkModeOverlay"></div>

